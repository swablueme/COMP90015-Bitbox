 //CompletableFuture<String> future = CompletableFuture.supplyAsync(() ->testAsync(fileSystemManager));
    public String testAsync(FileSystemManager fileSystemManage) {
        while (true) {
            ArrayList<FileSystemEvent> pathevents = fileSystemManager.generateSyncEvents();

            for (FileSystemEvent event : pathevents) {
                System.out.println(event);
                System.out.println(event.fileDescriptor.lastModified);
            }
        }
        return "a";

    }

-----
    public void testFileExists() {
        System.out.println(fileSystemManager.fileNameExists("ahhh.txt"));
    }

    public void testFileWritten() {
        try {
            System.out.println(fileSystemManager.checkWriteComplete("ahhh.txt"));

        } catch (Exception e) {
            exceptionHandler.handleException(e);
        }

    }
