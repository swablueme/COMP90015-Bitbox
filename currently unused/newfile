 public static void createSocket() {
        Socket clientSock = null;
        ServerSocket serverSock = null;
        try {
            clientSock = new Socket("localhost", 4444);
            serverSock = new ServerSocket(4444);
            System.out.println("");
            // Get the input/output streams for reading/writing data from/to the socket
            BufferedReader in = new BufferedReader(new InputStreamReader(clientSock.getInputStream(), "UTF-8"));
            BufferedWriter out = new BufferedWriter(new OutputStreamWriter(clientSock.getOutputStream(), "UTF-8"));
        } catch (UnknownHostException e) {
            e.printStackTrace();
        } catch (IOException e) {
            e.printStackTrace();
        } finally {
            if (clientSock != null) {
                try {
                    clientSock.close();
                } catch (IOException e) {
                    e.printStackTrace();
                }
            }
        }
    }
    
